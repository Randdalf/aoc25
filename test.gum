import io

test_counter = {}

fn test(day, part, solver, example, expected):
    input = io.read_file_text("examples/d{day:02d}_e{example}.txt")
    actual = solver(input)
    key = (day, part)
    if key in test_counter:
        test_counter[key] += 1
    else:
        test_counter[key] = 1
    id = "D{day:02d}P{part} #{test_counter[key]}"
    if actual != expected:
        print("FAIL {id}: Expected \"{expected}\", but got \"{actual}\".")
    else:
        print("PASS {id}")

# Day 1
import d01
test(1, 1, d01.part1, 1, 3)
test(1, 2, d01.part2, 1, 6)

# Day 2
import d02
test(2, 1, d02.part1, 1, 1227775554)
